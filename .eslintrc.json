{
  "root": true,
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "no-unused-vars": [
      "error",
      {
        // Ignore arguments starting with _
        "argsIgnorePattern": "^_"
      }
    ]
  }
}